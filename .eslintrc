{
  "parserOptions": {
    "ecmaVersion": 9
  },
  "rules": {
    "prettier/prettier": ["error"],
    "no-unused-vars": ["error", { "args": "none", "ignoreRestSiblings": false }],
    "no-console": "off"
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "plugins": ["prettier", "mocha"],
  "extends": "eslint:recommended"
}
