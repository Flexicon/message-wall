{
  "parserOptions": {
    "ecmaVersion": 6
  },
  "rules": {
    "prettier/prettier": ["error"],
    "no-unused-vars": ["error", { "args": "none", "ignoreRestSiblings": false }]
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "plugins": ["prettier", "mocha"],
  "extends": "eslint:recommended"
}
